{
  "name": "qwik-chat",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": ">=18"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages deploy ./dist",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist --compatibility-flags=nodejs_als",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@auth/core": "0.30.0",
    "@builder.io/qwik": "1.5.7",
    "@builder.io/qwik-auth": "0.2.2",
    "@builder.io/qwik-city": "1.5.7",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^9.3.0",
    "eslint-plugin-qwik": "1.5.7",
    "prettier": "^3.3.2",
    "typescript": "5.4.5",
    "undici": "^6.19.1",
    "vite": "^5.3.1",
    "vite-tsconfig-paths": "^4.3.2",
    "wrangler": "^3.60.3"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20240614.0",
    "@formkit/auto-animate": "^0.8.2",
    "@iconify-json/carbon": "^1.1.35",
    "@iconify-json/ri": "^1.1.21",
    "@iconify-json/simple-icons": "^1.1.106",
    "@types/markdown-it": "^14.1.1",
    "@types/uuid": "^9.0.8",
    "@unocss/reset": "^0.61.0",
    "@unscatty/unocss-preset-daisy": "^1.0.0",
    "array-keyed-map": "^2.1.3",
    "daisyui": "^4.12.2",
    "eventsource-parser": "^1.1.2",
    "fzf": "^0.5.2",
    "highlight.js": "^11.9.0",
    "html-to-image": "^1.11.11",
    "jose": "^5.4.0",
    "katex": "^0.16.10",
    "markdown-it": "^14.1.0",
    "markdown-it-highlightjs": "^4.1.0",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-kbd": "^2.2.2",
    "sass": "^1.77.6",
    "unocss": "^0.61.0",
    "unocss-preset-extra": "^0.5.3",
    "uuid": "^10.0.0"
  }
}
